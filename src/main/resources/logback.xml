<configuration>
    <appender name="FILE" class="ch.qos.logback.core.FileAppender">
        <file>insuranced.log</file>
        <encoder>
            <pattern>%d [%thread] %-5level %logger{35} - %msg%n</pattern>
        </encoder>
    </appender>
    <root level="DEBUG">
        <appender-ref ref="FILE" />
    </root>
</configuration>